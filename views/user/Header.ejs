<body>

  <div class="dropdown-menu" aria-labelledby="userDropdown">
    <a class="dropdown-item" href="/user-profile">Profile</a>
    <!-- Include onclick event to trigger SweetAlert -->
    <a id="logoutButton" class="dropdown-item logoutLink" style="color: red" href="/logout" onclick="confirmLogout(event)">Logout</a>
</div>

<script>
    function confirmLogout(event) {
        event.preventDefault(); // Prevent the default link behavior

        Swal.fire({
            title: 'Are you sure?',
            text: "You won't be able to revert this!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, logout!'
        }).then((result) => {
            if (result.isConfirmed) {
                // Redirect to the logout URL after the confirmation
                window.location.href = event.target.href;
            }
        });
    }
</script>